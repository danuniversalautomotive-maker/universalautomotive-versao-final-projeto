	"flex-layout.row#catalogo-individuais": {
		"title": "Catálogos Individuais Título",
		"children": [
			"rich-text#catalogo-individuais"
		]
	},
	"rich-text#catalogo-individuais": {
		"title": "Texto - Catálogos Individuais",
		"props": {
			"text": "# Catálogos individuais \n Escolha o catálogo digital de forma individual e faça o seu **download.**",
			"blockClass": [
				"catalogo-juntos",
				"catalogo-individuais"
			]
		}
	},
