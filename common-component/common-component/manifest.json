{
  "vendor": "universalautomotive",
  "name": "common-components",
  "version": "0.1.50",
  "title": "Universal Automotiva Common Custom Components",
  "description": "Universal Automotiva Common Custom Components",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x"
  },
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "dependencies": {
    "vtex.css-handles": "1.x",
    "vtex.formatted-price": "0.x",
    "vtex.format-currency": "0.x",
    "vtex.order-manager": "0.x",
    "vtex.product-context": "0.x",
    "vtex.store-components": "3.x",
    "vtex.styleguide": "9.x",
    "vtex.admin-graphql": "2.x",
    "vtex.store-graphql": "2.x",
    "vtex.b2b-organizations-graphql": "0.x",
    "vtex.storefront-permissions": "1.x",
    "vtex.inventory-graphql": "0.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/checkout/pub/*"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
