	"experimental__visibility-layout#whatsapp-button": {
		"title": "Imagem para Whatsapp (On/Off)",
		"children": [
			"flex-layout.row#whatsapp-button"
		],
		"props": {
			"visible": true
		}
	},
	"flex-layout.row#whatsapp-button": {
		"children": [
			"image#whatsapp-button"
		],
		"props": {
			"blockClass": "whatsapp-button",
			"fullWidth": true
		}
	},
	"image#whatsapp-button": {
		"title": "Imagem para Whatsapp",
		"props": {
			"blockClass": "whatsapp-button",
			"src": "assets/icons/whatsapp.svg",
			"link": {
				"url": "https://wa.me/1122487777",
				"openNewTab": true
			}
		}
	},
	"flex-layout.row#demo-store-warning": {
		"props": {
			"blockClass": "demo-store-warning",
			"fullWidth": true
		},
		"children": [
			"flex-layout.col#demo-store-warning"
		]
	},
	"flex-layout.col#demo-store-warning": {
		"children": [
			"rich-text#demo-store-warning"
		]
	},
	"rich-text#demo-store-warning": {
		"title": "Texto de aviso da loja de demonstração",
		"props": {
			"text": "Loja em desenvolvimento",
			"textPosition": "CENTER",
			"textColor": "c-action-primary",
			"blockClass": "demo-store-warning"
		}
	},
	"sticky-layout#desktop": {
		"props": {
			"blockClass": "sticky-header"
		},
