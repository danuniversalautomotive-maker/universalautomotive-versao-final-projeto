	"rich-text#headStripe1": {
		"title": "1º item da NavBar",
		"props": {
			"blockClass": "HeaderStripeText",
			"text": "[Lançamentos](/releases)"
		}
	},
	"rich-text#headStripe2": {
		"title": "2º item da NavBar",
		"props": {
			"blockClass": "HeaderStripeText",
			"text": "[Ofertas](/deals)"
		}
	},
	"rich-text#headStripe3": {
		"title": "3º item da NavBar",
		"props": {
			"blockClass": "HeaderStripeText",
