	"link#b2b-user-widget-link-2": {
		"props": {
			"label": "Minhas cotações",
			"href": "/b2b-quotes"
		}
	},
	"rich-text#b2b-user-widget-link-1": {
		"props": {
			"text": "Gerenciar organização",
			"textPosition": "CENTER",
			"link": {
				"url": "/account#/organization"
			}
		}
